local function foo(x: number, y: number): number
    return x + y
end

local function add(a: number, b: number): number
    return a + b
end

local result: number = add(1, 2)

local function divmod(a: number, b: number): number, number
    return a / b, a % b
end

local q: number, r: number = divmod(10, 3)

local function identity<T>(x: T): T
    return x
end

local a: number = identity(42)
local b: string = identity("hello")

local sn: string | number = identity("world")
local x: number | nil = 42
local y: number | nil = nil

interface Person
    name: string
    age: number
end

interface Animal
    name: string
    age: number
    speak: function
end

function create_dog(): Animal
    local dog = {
        name = "Buddy",
        age = 5,
        speak = function() end
    }
    return dog
end
